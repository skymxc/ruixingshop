/**app.wxss**/
page{
  width: 100%;
  height: 100%;
  background-color: ghostwhite;
}
.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  font-size: 15px;
  
}

.box {
  box-sizing: border-box;
}

.container-interval-vertical {
  /* margin-top: 5px; */
  margin-bottom: 5px;
}

/**白色矩形框**/

.container-landscape-white {
  width: 100%;
  min-height: 40px;
  background-color: white;
}

.contianer-box-center{
  padding: 5px;
  display: flex;
  align-items: center;
}
.container-underline{
  border-bottom: solid 1px rgba(128, 128, 128, 0.212);
}

.mask {
  width: 100%;
  height: 100%;
  background-color: rgba(7, 0, 0, 0.404);
  position: fixed;
  top: 0px;
}